<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Publisher:</strong>
    <%= book.publisher %>
  </p>

  <p>
    <strong>Publication year:</strong>
    <%= book.publication_year %>
  </p>

  <p>
    <strong>Shelf:</strong>
    <%= book.shelf %>
  </p>

  <p>
    <strong>Quanty:</strong>
    <%= book.quanty %>
  </p>

</div>
