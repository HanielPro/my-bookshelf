<div id="<%= dom_id user %>">
  <p>
    <strong>Nome:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Habilitado:</strong>
    <%= user.habilitaded %>
  </p>

  <p>
    <strong>Tipo:</strong>
    <%= user.role %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Senha:</strong>
    <%= user.password %>
  </p>

</div>
